{
  "name": "@warpio/warpio-net",
  "version": "1.0.0",
  "description": "Web interface for Warpio CLI - Terminal and File Management",
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JaimeCernuda/warpio-net.git"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:terminal\" \"npm run dev:frontend\"",
    "dev:terminal": "cd packages/web-terminal && npm run dev",
    "dev:frontend": "cd packages/terminal-frontend && npm run dev",
    "build": "npm run build:frontend && npm run build:terminal",
    "build:frontend": "cd packages/terminal-frontend && npm run build",
    "build:terminal": "cd packages/web-terminal && npm run build",
    "start": "cd packages/web-terminal && npm start",
    "install-deps": "npm install && npm install --workspaces",
    "clean": "npm run clean --workspaces --if-present"
  },
  "keywords": [
    "warpio",
    "web-terminal",
    "file-manager",
    "authentication",
    "ai-cli"
  ],
  "author": "Jaime Cernuda",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^9.2.0",
    "typescript": "^5.7.3"
  }
}